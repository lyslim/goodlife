<ul class="navigation primary">
    <li class="<%= active_li_css(@view_by, "All") %>">
        <small><%= link_to_unless((@view_by == "All"), "All", :controller => 'feats', :action => 'plans', :type=>"All") %></small>
    </li>
  <% PlannedTodo::TYPES.each do |type| %>
    <li class="<%= active_li_css(@view_by, type) %>">
			<small><%=
				link_to_unless(
				  (@view_by == type),
					type,
					:controller => 'feats',
					:action => 'plans',
					:type => type
				)
			%></small>
		</li>
	<% end %>
</ul>
<div class="clear"></div>