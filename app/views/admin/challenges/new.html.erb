<div class="row">
  <div class="column grid_10">
    <h1>New Challenge</h1>
    <p><a href="/admin/challenges">&laquo; Back to Challenge List</a></p>
    <%= semantic_form_for @challenge, :url => {:action => "create"}, :html => { :multipart => true } do |f| %>
      <%= f.inputs :name, :description, :feats, :start_on, :end_on, :bonus_points %>
      <%= f.buttons :commit %>
    <% end %>
  </div>
  <%# render :partial => 'crud_right' %>
</div>